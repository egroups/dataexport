object TexExporter
  Description = 'Orders data exportt'
  Sessions = <  
    item
      Name = 'invoices_table'
      provider = 'invoices-provider'      
      Columns = <                
        item
          Name = 'number'          
        end
        item
          Name = 'created_at'
          Expression = 'Result := FormatDateTime(''yyyy-mm-dd'', Value);'
          Dictionary = 'quoted'
        end        
        item
          Name = 'description'
          Dictionary = 'quoted'
        end> 
      Sessions = <
        item
          Name = 'details_table'
          provider = 'details-provider'
          Columns = <                         
            item
              Name = 'product_id'              
            end
            item
              Name = 'quantity'              
            end
            item
              Name = 'price'              
            end
            item
              Alias = 'value' 
              Name = 'total'        
            end>
          Sessions = <>
        end>      
    end>
  Dictionaries = <  
    item
      Name = 'quoted'            
      Expression = 'Result := QuotedStr(Value);'      
    end>    
  SerializerClassName = 'TexSQLInsertSerializer'    
  Packages = <    
    item
      Name = 'invoices.sql'
      Sessions.Strings = (
        'invoices_table'          
      )    
    end> 
  Providers = <
    item
      Name = 'invoices-provider'
      SQL.Strings = (
	      'select * from invoices'
	    )
    end
    item
      Name = 'details-provider'
      SQL.Strings = (
	      'select * from invoice_details where invoice_id = :id'
	    )
    end> 
end